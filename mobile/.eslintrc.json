{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "class-methods-use-this": [0],
        "no-underscore-dangle": [0],
        "react/sort-comp": [0],
        "no-nested-ternary": [0]
    }
}