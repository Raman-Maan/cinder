{
  "name": "cinder",
  "version": "0.1.0",
  "scripts": {
    "web": "cd ../web && npm start && npm run watch-css",
    "server": "nodemon server.js",
    "dev": "NODE_ENV=development concurrently --kill-others-on-fail \"npm run server\" \"npm run web\"",
    "test": "NODE_ENV=test node db/utils/build_db.js && jest",
    "precommit": "lint-staged",
    "build:testdata": "node db/utils/generateTestData.js",
    "db:dev:build": "NODE_ENV=development node db/utils/build_db.js",
    "db:dev:addTestData": "NODE_ENV=development node db/utils/add_test_data.js",
    "db:dev": "NODE_ENV=development node db/utils/build_db.js && node db/utils/add_test_data.js"
  },
  "description": "",
  "main": "server.js",
  "author": "COMP4350 Group 7",
  "license": "ISC",
  "lint-staged": {
    "**/*.{js,jsx,json,scss}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "dependencies": {
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "mysql": "^2.15.0",
    "promise-mysql": "^3.2.0",
    "require-sql": "^1.0.0"
  },
  "devDependencies": {
    "cheerio": "^1.0.0-rc.2",
    "concurrently": "^3.5.1",
    "eslint": "^4.16.0",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "lint-staged": "^6.0.0",
    "nodemon": "^1.14.11",
    "prettier": "^1.10.2",
    "request": "^2.83.0",
    "request-promise": "^4.2.2"
  }
}
